@using Wordle.Areas.Identity.Data
@model List<Score>
@{
    <table class="table table-striped table-dark">
      <thead>
        <tr>
		  <th scope="col">Date</th>
          <th scope="col">Mot</th>
          <th scope="col">Essais</th>
          <th scope="col">Points</th>
        </tr>
      </thead>
      <tbody>
	    @foreach(Score score in Model)
	    {
			<tr>
		        <td>@score.Date</td>
		        <td>@score.Word</td>
		        <td>@score.Tries</td>
		        <td>@score.ScorePoints</td>
            </tr>
	    }
      </tbody>
    </table>
}
